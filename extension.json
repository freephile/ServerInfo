{
	"name": "ServerInfo",
	"author": [
		"Greg Rundlett"
	],
	"url": "https://github.com/freephile/ServerInfo",
	"descriptionmsg": "serverinfo-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\ServerInfo\\": "includes/"
	},
	"HookHandlers": {
		"ServerInfoHooks": {
			"class": "MediaWiki\\Extension\\ServerInfo\\Hooks"
		}
	},
	"MessagesDirs": {
		"ServerInfo": [
			"i18n"
		]
	},
	"AvailableRights": [
		"viewserverinfo"
	],
	"GroupPermissions": {
		"sysop": {
			"viewserverinfo": true
		},
		"user": {
			"viewserverinfo": false
		},
		"*": {
			"viewserverinfo": false
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "ServerInfo/resources"
	},
	"ResourceModules": {
		"ext.serverInfo": {
			"dependencies": [],
			"messages": [],
			"styles": [],
			"scripts": []
		}
	},
	"manifest_version": 2
}
